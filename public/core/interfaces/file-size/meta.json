{
  "name": "file-size",
  "version": "1.0.0",
  "dataTypes": {
    "INT": 11,
    "TINYINT": 1,
    "SMALLINT": 5,
    "MEDIUMINT": 7,
    "BIGINT": 18
  },
  "options": {
    "placeholder": {
      "interface": "text-input",
      "comment": "Enter placeholder text",
      "defaultValue": "",
      "length": 200
    },
    "formatValue": {
      "interface": "toggle",
      "comment": "Show the value human readable (e.g. 10MB)",
      "defaultValue": true
    },
    "useDecimal": {
      "interface": "toggle",
      "comment": "Show the value in decimal counting (10MB vs 10MiB)",
      "defaultValue": true
    }
  }
}
