{
  "name": "textarea",
  "version": "1.0.0",
  "dataTypes": {
    "TEXT": null,
    "TINYTEXT": null,
    "MEDIUMTEXT": null,
    "LONGTEXT": null,
    "VARCHAR": 255
  },
  "options": {
    "rows": {
      "interface": "numeric",
      "comment": "The number of text rows available for the input before scrolling",
      "options": {
        "min": 5,
        "max": 100
      },
      "defaultValue": 12
    },
    "placeholder": {
      "interface": "text-input",
      "comment": "Enter placeholder text",
      "defaultValue": "",
      "length": 200
    }
  }
}
