{
  "name": "$t:markdown",
  "version": "1.0.0",
  "datatypes": {
    "VARCHAR": 1024,
    "TINYTEXT": null,
    "TEXT": null,
    "MEDIUMTEXT": null,
    "LONGTEXT": null
  },
  "options": {
    "rows": {
      "name": "$t:rows",
      "comment": "$t:rows_comment",
      "interface": "numeric",
      "options": {
        "min": 5,
        "max": 100
      },
      "default": 12
    },
    "placeholder": {
      "name": "$t:placeholder",
      "comment": "$t:placeholder_comment",
      "interface": "textarea",
      "default": "Enter **markdown** here...",
      "placeholder": "Enter **markdown** here..."
    }
  },
  "translation": {
    "en-US": {
      "markdown": "Markdown",
      "rows": "Rows",
      "rows_comment": "The starting number of text rows available",
      "placeholder": "Placeholder",
      "placeholder_comment": "Enter placeholder text"
    }
  }
}
